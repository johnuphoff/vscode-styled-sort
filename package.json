{
    "name": "srt",
    "displayName": "srt",
    "description": "styled components sass rule ordering",
    "version": "0.0.1",
    "publisher": "ease",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:javascript"
    ],
    "main": "./out/extension",
    "contributes": {
        "languages": [
            {
                "id": "javascript",
                "aliases": [
                    "Javascript"
                ],
                "extensions": [
                    "javascript"
                ]
            }
        ],
        "commands": [
            {
                "command": "extension.format-foo",
                "title": "(Bad) Format Foo Files"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
